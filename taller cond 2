(define (principal4)
  (begin
    (display "Numero 1: ")(define a (read))
    (display "Numero 2: ")(define b (read))
    (display "Numero 3: ")(define c (read))
    (display "Numero 4: ")(define d (read))
    (display "Numero 5: ")(define f (read))

    (display "Numero final: ")(menormayor a b c d f)))






                         
